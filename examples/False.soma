(f False) isNotNil => True.

(f False) isNil => False.

(f False) ifNil: nBlock => Nil.

(f False) ifNotNil: nBlock => nBlock value.

(f False) ifFalse: fBlock => fBlock value.

(f False) ifTrue: tBlock => Nil.

(f False) ifTrue: tBlock ifFalse: fBlock => 
   fBlock value.

(f False) not => True.

(f False) & aBool => f.

(f False) | aBool =>
   aBool ifTrue:  { True } ifFalse: { f }.

(f False) ^ aBool =>
   aBool ifTrue: { True } ifFalse: { f }.
