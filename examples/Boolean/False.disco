+ False ifNotNil => { True }

+ False ifNotNilDo: nBlock => { nBlock value }

+ False ifFalse: fBlock => { fBlock value }

+ False ifTrue: tBlock => { Nil }

+ False ifTrue: tBlock ifFalse: fBlock => { fBlock value }

+ False not => { True }

+ False & aBool => { False }

+ False | aBool => {
  aBool ifTrue:  { True } ifFalse: { False }
}

+ False ^ aBool => {
  aBool ifTrue: { True } ifFalse: { False }
}